<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:drawable="@drawable/bg_circle_selected" android:state_selected="true" app:tsquare_state_range_first="false" app:tsquare_state_range_last="false" app:tsquare_state_range_middle="false" />
    <item android:drawable="@drawable/bg_first_selected" android:state_selected="true" app:tsquare_state_range_first="true" />
    <item android:drawable="@drawable/bg_middle_selected" android:state_selected="true" app:tsquare_state_range_middle="true" />
    <item android:drawable="@drawable/bg_last_selected" android:state_selected="true" app:tsquare_state_range_last="true" />
    <!--<item android:state_pressed="true" android:drawable="@drawable/bg_circle_selected"/>-->
    <item app:tsquare_state_current_month="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/custom_background_disabled" />
        </shape>
    </item>
    <item android:drawable="@drawable/bg_circle_today" app:tsquare_state_today="true" />
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/custom_background" />
        </shape>
    </item>
</selector>
